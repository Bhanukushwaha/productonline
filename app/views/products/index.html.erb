<h3>product</h3>

<table>
	<thead>
		<tr>
			<th>name</th>
			<th>description</th>
			<th>price</th>
		</tr>
	</thead>
	<tbody>
		<% @products.each do |product| %>
			<tr>
				<td><%= product.name %></td>
				<td><%= product.description %></td>
				<td><%= product.price %></td>
			</tr>
		<% end %>
	</tbody>
</table>
