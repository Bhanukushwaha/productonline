<div class="container">
  <%= link_to 'Beck', :back, class: "btn btn-outline-info" %>

  <%= form_with model: @address do |f| %>
  <div class="form-group row">
    <label for="inputCountry" class="col-sm-2 col-form-label">Country</label>
    <div class="col-sm-10">      
      <%= f.text_field :country, placeholder: 'Enter Country Name',class: "form-control" %>
    </div>
  </div>

  <div class="form-group row">
    <label for="inputDistrict" class="col-sm-2 col-form-label">District</label>
    <div class="col-sm-10">
      <%= f.text_field :district, placeholder: 'Enter district Name', class: "form-control" %>
    </div>
  </div>

  <div class="form-group row">
    <label for="inputPost_code" class="col-sm-2 col-form-label">Post_code</label>
    <div class="col-sm-10">
      <%= f.text_field :post_code, placeholder: 'Enter  Post code', class: "form-control" %>
    </div>
  </div>

  <div class="form-group row">
    <label for="inputAddress" class="col-sm-2 col-form-label">Address</label>
    <div class="col-sm-10">      
      <%= f.text_field :address, placeholder: 'Enter your address', class: "form-control" %>
    </div>
  </div>

  <div class="form-group row">    
    <%= f.submit class: "btn btn-outline-secondary btn-lg" %> 
  </div>
<% end %>
</div>


  
  