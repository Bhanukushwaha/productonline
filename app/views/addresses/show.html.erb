<div class="container">
  <p id="notice"><%= notice %></p>
  <%= link_to 'Back', addresses_path, class: "btn btn-outline-info" %>
  <form>
    <div class="form-group row">
      <label for="staticCountry" class="col-sm-2 col-form-label">Country</label>
      <div class="col-sm-10">
        <%= @address.country %>
      </div>
    </div>
    <div class="form-group row">
      <label for="staticPost_code" class="col-sm-2 col-form-label">Post_code</label>
      <div class="col-sm-10">
        <%= @address.post_code %>
      </div>
    </div>
    <div class="form-group row">
      <label for="staticDistrict" class="col-sm-2 col-form-label">District</label>
      <div class="col-sm-10">
        <%= @address.district %>
      </div>
    </div>
    <div class="form-group row">
      <label for="staticAddress" class="col-sm-2 col-form-label">Address</label>
      <div class="col-sm-10">
        <%= @address.address %>
      </div>
    </div>
  </form>  
  <%#= link_to 'Edit', edit_address_path(@address), class: "btn btn-outline-success" %>
</div>
